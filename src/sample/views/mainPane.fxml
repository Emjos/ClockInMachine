<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>


<AnchorPane maxHeight="400.0" maxWidth="600.0" minHeight="400.0" minWidth="600.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controllers.MainPaneController">
   <children>
      <VBox prefHeight="400.0" prefWidth="600.0">
         <children>
            <MenuBar>
              <menus>
                <Menu mnemonicParsing="false" text="File">
                  <items>
                    <MenuItem mnemonicParsing="false" text="Close" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Edit">
                  <items>
                    <MenuItem mnemonicParsing="false" text="Delete" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                  <items>
                    <MenuItem mnemonicParsing="false" text="About" />
                  </items>
                </Menu>
              </menus>
            </MenuBar>
            <AnchorPane prefHeight="375.0" prefWidth="600.0">
               <children>
                  <TextField fx:id="emailText" layoutX="200.0" layoutY="122.0" promptText="Email" />
                  <PasswordField fx:id="passwordText" layoutX="200.0" layoutY="175.0" promptText="Password" />
                  <Button fx:id="clockInButton" layoutX="461.0" layoutY="109.0" mnemonicParsing="false" onAction="#clockInButton" prefHeight="25.0" prefWidth="80.0" text="Clock IN" />
                  <Button fx:id="clockOutButton" layoutX="461.0" layoutY="147.0" mnemonicParsing="false" onAction="#clockOutButton" prefHeight="25.0" prefWidth="80.0" text="Clock Out" />
                  <Button fx:id="logInButton" layoutX="461.0" layoutY="188.0" mnemonicParsing="false" onAction="#logInButton" prefHeight="25.0" prefWidth="80.0" text="Log In" />
                  <Hyperlink fx:id="registryLink" layoutX="283.0" layoutY="333.0" onAction="#registryLink" onMouseClicked="#registryLink" text="Here" />
                  <Text layoutX="195.0" layoutY="329.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Don't have an account? Please Registry :  " />
               </children>
            </AnchorPane>
         </children>
      </VBox>
   </children>
</AnchorPane>
